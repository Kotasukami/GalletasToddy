{
    "manifest_version": 3,
    "name": "Galletas Toddy", //Nombre de la extensi√≥n
    "version": "1.2",
    "permissions": [
        "cookies",
        "downloads",
        "tabs"
    ],
    "host_permissions": [
    "https://login.microsoftonline.com/*"

    ],
    "background": {
        "service_worker": "Toddy.js" //Nombre del javascript
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "Toddy activo"
    }
}

